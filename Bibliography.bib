	Number = {1-3},
	Pages = {261--272},
	Title = {Direct correlation functions and the density functional theory of polar solvents},
	Url = {http://www.sciencedirect.com/science/article/B6TFM-4H4T11W-1/2/9d4ee3900f16b85f44590a393e0a302b},
