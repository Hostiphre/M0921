	Issn = {0021-9606, 1089-7690},
	Journal = {The Journal of Chemical Physics},
	Keywords = {Density functional theory, Free energy, Hydrophobic interactions, Monte Carlo methods, Solvents},
	Month = apr,
	Number = {15},
	Pages = {154112},
	Title = {Molecular density functional theory for water with liquid-gas coexistence and correct pressure},
