	Journal = {Physical Review E},
	Number = {3},
	Pages = {031206--031206--8},
	Shorttitle = {Density functional theory of solvation in a polar solvent},
	Title = {Density functional theory of solvation in a polar solvent: {Extracting} the functional from homogeneous solvent simulations},
	Url = {http://link.aps.org/doi/10.1103/PhysRevE.66.031206},
