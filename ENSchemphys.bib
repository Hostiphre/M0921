@article{calandra_nuclear_2002,
	title = {Nuclear {Quadrupole} {Coupling} of 17O and 33S in {Ionic} {Solids}:  {Invalidation} of the {Sternheimer} {Model} by {Short}-{Range} {Corrections}},
