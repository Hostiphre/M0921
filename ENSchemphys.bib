	file = {APS Full Text PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/26W3II9T/Camboni et al. - 2013 - Diffusion of small particles in a solid polymeric .pdf:application/pdf;APS Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/CEJTVTBG/e022120.html:text/html}
}

