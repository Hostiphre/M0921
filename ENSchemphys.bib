@article{bacri_dynamics_2011,
	title = {Dynamics of {Colloids} in {Single} {Solid}-{State} {Nanopores}},
	volume = {115},
	issn = {1520-6106},
	url = {http://dx.doi.org/10.1021/jp200326w},
	doi = {10.1021/jp200326w},
	abstract = {We use solid-state nanopores to study the dynamics of single electrically charged colloids through nanopores as a function of applied voltage. We show that the presence of a single colloid inside of the pore changes the pore resistance, in agreement with theory. The normalized ionic current blockade increases with the applied voltage and remains constant when the electrical force increases even more. We observe short and long events of current blockades. Their durations are associated, respectively, with low and high current variation. The ratio of long events increases with the electrical force. The events frequency increases exponentially as a function of applied voltage and saturates at high voltage. The dwelling time decreases exponentially at low and medium voltages when the electrical force increases. At large voltages, this time decreases inversely proportionally to the applied voltage. The long events are associated with translocation events. We show that the dynamics of colloids through the nanopore is governed mainly by two mechanisms, by the free-energy barrier at relatively low and medium voltages and by the electrophoresis mechanism at high voltage.},
	number = {12},
	urldate = {2013-08-23},
	journal = {The Journal of Physical Chemistry B},
	author = {Bacri, L. and Oukhaled, A. G. and Schiedt, B. and Patriarche, G. and Bourhis, E. and Gierak, J. and Pelta, J. and Auvray, L.},
	month = mar,
	year = {2011},
	pages = {2890--2898},
	file = {ACS Full Text PDF w/ Links:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/B6PI48UC/Bacri et al. - 2011 - Dynamics of Colloids in Single Solid-State Nanopor.pdf:application/pdf;ACS Full Text Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/9FKPAZ55/jp200326w.html:text/html}
}

@article{cleland_taming_2012,
	title = {Taming the {First}-{Row} {Diatomics}: {A} {Full} {Configuration} {Interaction} {Quantum} {Monte} {Carlo} {Study}},
	volume = {8},
	issn = {1549-9618},
	shorttitle = {Taming the {First}-{Row} {Diatomics}},
	url = {http://dx.doi.org/10.1021/ct300504f},
	doi = {10.1021/ct300504f},
	abstract = {The initiator full configuration interaction quantum Monte Carlo (i-FCIQMC) method has recently been developed as a highly accurate stochastic electronic structure technique. It has been shown to calculate the exact basis-set ground state energy of small molecules, to within modest stochastic error bars, using tractable computational cost. Here, we use this technique to elucidate an often troublesome series of first-row diatomics consisting of Be2, C2, CN, CO, N2, NO, O2, and F2. Using i-FCIQMC, the dissociation energies of these molecules are obtained almost entirely to within chemical accuracy of experimental results. Furthermore, the i-FCIQMC calculations are performed in a relatively black-box manner, without any a priori knowledge or specification of the wave function. The size consistency of i-FCIQMC is also demonstrated with regards to these diatomics at their more multiconfigurational stretched geometries. The clear and simple i-FCIQMC wave functions obtained for these systems are then compared and investigated to demonstrate the dynamic identification of the dominant determinants contributing to significant static correlation. The appearance and nature of such determinants is shown to provide insight into both the i-FCIQMC algorithm and the diatomics themselves.},
	number = {11},
	urldate = {2015-05-12},
	journal = {Journal of Chemical Theory and Computation},
	author = {Cleland, Deidre and Booth, George H. and Overy, Catherine and Alavi, Ali},
	month = nov,
	year = {2012},
	pages = {4138--4152},
	file = {ACS Full Text PDF w/ Links:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/D8HP4P7B/Cleland et al. - 2012 - Taming the First-Row Diatomics A Full Configurati.pdf:application/pdf;ACS Full Text Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/9PP98HJH/ct300504f.html:text/html}
}

@article{thomas_stochastic_2015,
	title = {Stochastic {Multiconfigurational} {Self}-{Consistent} {Field} {Theory}},
	volume = {11},
	issn = {1549-9618},
	url = {http://dx.doi.org/10.1021/acs.jctc.5b00917},
	doi = {10.1021/acs.jctc.5b00917},
	abstract = {The multiconfigurational self-consistent field theory is considered the standard starting point for almost all multireference approaches required for strongly correlated molecular problems. The limitation of the approach is generally given by the number of strongly correlated orbitals in the molecule, since its cost will grow exponentially with this number. We present a new multiconfigurational self-consistent field approach, wherein linear determinant coefficients of a multiconfigurational wave function are optimized via the stochastic full configuration interaction quantum Monte Carlo technique at greatly reduced computational cost, with nonlinear orbital rotation parameters updated variationally based on this sampled wave function. This extends this approach to strongly correlated systems with far larger active spaces than it is possible to treat via conventional means. By comparison with this traditional approach, we demonstrate that the introduction of stochastic noise in both the determinant amplitudes and the gradient and Hessian of the orbital rotations does not preclude robust and reliable convergence of the orbital optimization. It can even improve the ability to avoid convergence to local minima in the orbital space, and therefore aid in finding variationally lower-energy solutions. We consider the effect on the convergence of the orbitals as the number of walkers and the sampling time within the active space increases, as well as the effect on the final energy and error. The scope of the new protocol is demonstrated with a study of the increasingly strongly correlated electronic structure in a series of polycyclic aromatic hydrocarbons, up to the large coronene molecule in a complete active space of 24 π electrons in 24 orbitals, requiring only modest computational resources.},
	number = {11},
	urldate = {2016-04-20},
	journal = {Journal of Chemical Theory and Computation},
	author = {Thomas, Robert E. and Sun, Qiming and Alavi, Ali and Booth, George H.},
	month = nov,
	year = {2015},
	pages = {5316--5325},
	file = {ACS Full Text PDF w/ Links:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/BW2QJFXR/Thomas et al. - 2015 - Stochastic Multiconfigurational Self-Consistent Fi.pdf:application/pdf;ACS Full Text Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/KDPZIAW5/acs.jctc.html:text/html}
}

@article{shi_gradient-related_2004,
	title = {A gradient-related algorithm with inexact line searches},
	volume = {170},
	issn = {0377-0427},
	url = {http://www.sciencedirect.com/science/article/pii/S0377042704000858},
	doi = {10.1016/j.cam.2003.10.025},
	abstract = {In this paper, a new gradient-related algorithm for solving large-scale unconstrained optimization problems is proposed. The new algorithm is a kind of line search method. The basic idea is to choose a combination of the current gradient and some previous search directions as a new search direction and to find a step-size by using various inexact line searches. Using more information at the current iterative step may improve the performance of the algorithm. This motivates us to find some new gradient algorithms which may be more effective than standard conjugate gradient methods. Uniformly gradient-related conception is useful and it can be used to analyze global convergence of the new algorithm. The global convergence and linear convergence rate of the new algorithm are investigated under diverse weak conditions. Numerical experiments show that the new algorithm seems to converge more stably and is superior to other similar methods in many situations.},
	number = {2},
