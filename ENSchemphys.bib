@article{lupi_hydration_2012,
	title = {Hydration and aggregation in mono- and disaccharide aqueous solutions by gigahertz-to-terahertz light scattering and molecular dynamics simulations.},
	volume = {116},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/23205713},
	doi = {10.1021/jp3079869},
	abstract = {The relaxation properties of hydration water around fructose, glucose, sucrose, and trehalose molecules have been studied by means of extended frequency range depolarized light scattering and molecular dynamics simulations. Evidence is given of hydration dynamics retarded by a factor ξ = 5-6 for all the analyzed solutes. A dynamical hydration shell is defined based on the solute-induced slowing down of water mobility at picosecond time scales. The number of dynamically perturbed water molecules N(h) and its concentration dependence have been determined in glucose and trehalose aqueous solutions up to high solute weight fractions (ca. 45\%). For highly dilute solutions, about 3.3 water molecules per sugar hydroxyl group are found to be part of the hydration shell of mono- and disaccharide. For increasing concentrations, a noticeable solute-dependent reduction of hydration number occurs, which has been attributed, in addition to simple statistical shells overlapping, to aggregation of solute molecules. A scaling law based on the number of hydroxyl groups collapses the N(h) concentration dependence of glucose and trehalose into a single master plot, suggesting hydration and aggregation properties independent of the size of the sugar. As a whole, the present results point to the concentration of hydroxyl groups as the parameter guiding both sugar-water and sugar-sugar interactions, without appreciable difference between mono- and disaccharides.},
	number = {51},
	journal = {The Journal of Physical Chemistry B},
	author = {Lupi, L and Comez, L and Paolantoni, M and Perticaroli, S and Sassi, P and Morresi, A and Ladanyi, B M and Fioretto, D},
	month = dec,
	year = {2012},
	keywords = {Disaccharides, Disaccharides: chemistry, Glucose, Glucose: chemistry, Light, Molecular Dynamics Simulation, Radiation, Scattering, Sucrose, Sucrose: chemistry, Trehalose, Trehalose: chemistry, water, Water: chemistry},
	pages = {14760--14767},
	file = {Lupi et al. - 2012 - Hydration and aggregation in mono- and disaccharide aqueous solutions by gigahertz-to-terahertz light scattering an:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/AHCQQI4I/Lupi et al. - 2012 - Hydration and aggregation in mono- and disaccharide aqueous solutions by gigahertz-to-terahertz light scattering an.pdf:application/pdf}
