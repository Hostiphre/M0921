@article{ceperley_ground_1978,
	title = {Ground state of the fermion one-component plasma: {A} {Monte} {Carlo} study in two and three dimensions},
	volume = {18},
	shorttitle = {Ground state of the fermion one-component plasma},
	url = {http://link.aps.org/doi/10.1103/PhysRevB.18.3126},
	doi = {10.1103/PhysRevB.18.3126},
	abstract = {We have performed fermion Monte Carlo variational calculations to determine the equation of state of the uniform electron one-component plasma in two and three dimensions. The ground-state excess energies calculated by the Monte Carlo method are very precise and in agreement with those of other calculations in the metallic density range and in the very-low-density Wigner crystals. Three phases have been investigated: the Wigner crystal, the normal or unpolarized fluid, and the polarized fluid. The Wigner crystal has the lowest energy for rs{\textgreater}67 in three dimensions and rs{\textgreater}33 in two dimensions. The totally polarized quantum fluid is stable for 26{\textless}rs{\textless}67 in three dimensions and for 13{\textless}rs{\textless}33 in two dimensions, and the normal or unpolarized fluid is stable at higher densities rs{\textless}26 in three dimensions and rs{\textless}13 in two dimensions. A pseudopotential with no adjustable parameters, derived from the random-phase approximation, is found to give excellent energies. The present results lend support to earlier conjectures that the ground state of the electron gas will be spin polarized at intermediate densities.},
	number = {7},
	urldate = {2010-04-08},
	journal = {Physical Review B},
	author = {Ceperley, D.},
	month = oct,
	year = {1978},
	pages = {3126},
	file = {APS Full Text PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/E7ECAS5N/Ceperley - 1978 - Ground state of the fermion one-component plasma .pdf:application/pdf}
}

@article{carvalho_specific_2009,
