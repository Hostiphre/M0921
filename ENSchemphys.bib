@article{mallamace_nmr_2008,
	title = {{NMR} evidence of a sharp change in a measure of local order in deeply supercooled confined water.},
	volume = {105},
	url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2526099&tool=pmcentrez&rendertype=abstract},
	doi = {10.1073/pnas.0805032105},
	abstract = {Using NMR, we measure the proton chemical shift delta, of supercooled nanoconfined water in the temperature range 195 K {\textless} T {\textless} 350 K. Because delta is directly connected to the magnetic shielding tensor, we discuss the data in terms of the local hydrogen bond geometry and order. We argue that the derivative -( partial differential ln delta/ partial differentialT)(P) should behave roughly as the constant pressure specific heat C(P)(T), and we confirm this argument by detailed comparisons with literature values of C(P)(T) in the range 290-370 K. We find that -( partial differential ln delta/ partial differentialT)(P) displays a pronounced maximum upon crossing the locus of maximum correlation length at approximately 240 K, consistent with the liquid-liquid critical point hypothesis for water, which predicts that C(P)(T) displays a maximum on crossing the Widom line.},
