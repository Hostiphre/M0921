	pages = {161102--161102--4},
	file = {AIP Journal PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/A9XEHHXP/Turton et al. - 2008 - Glasslike behavior in aqueous electrolyte solution.pdf:application/pdf;AIP Journal Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/SQSNVWFP/p161102_s1.html:text/html}
