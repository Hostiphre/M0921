@article{lerbret_low-frequency_2009,
	title = {Low-frequency vibrational properties of lysozyme in sugar aqueous solutions: a {Raman} scattering and molecular dynamics simulation study.},
	volume = {131},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/20059115},
	doi = {10.1063/1.3273218},
	abstract = {The low-frequency (omega{\textless}400 cm(-1)) vibrational properties of lysozyme in aqueous solutions of three well-known protecting sugars, namely, trehalose, maltose, and sucrose, have been investigated by means of complementary Raman scattering experiments and molecular dynamics simulations. The comparison of the Raman susceptibility chi(")(omega) of lysozyme/water and lysozyme/sugar/water solutions at a concentration of 40 wt \% with the chi(") of dry lysozyme suggests that the protein dynamics mostly appears in the broad peak around 60-80 cm(-1) that reflects the vibrations experienced by atoms within the cage formed by their neighbors, whereas the broad shoulder around 170 cm(-1) mainly stems from the intermolecular O-H...O stretching vibrations of water. The addition of sugars essentially induces a significant high frequency shift and intensity reduction of this band that reveal a slowing down of water dynamics and a distortion of the tetrahedral hydrogen bond network of water, respectively. Furthermore, the lysozyme vibrational densities of states (VDOS) have been determined from simulations of lysozyme in 37-60 wt \% disaccharide aqueous solutions. They exhibit an additional broad peak around 290 cm(-1), in line with the VDOS of globular proteins obtained in neutron scattering experiments. The influence of sugars on the computed VDOS mostly appears on the first peak as a slight high-frequency shift and intensity reduction in the low-frequency range (omega{\textless}50 cm(-1)), which increase with the sugar concentration and with the exposition of protein residues to the solvent. These results suggest that sugars stiffen the environment experienced by lysozyme atoms, thereby counteracting the softening of protein vibrational modes upon denaturation, observed at high temperature in the Raman susceptibility of the lysozyme/water solution and in the computed VDOS of unfolded lysozyme in water. Finally, the Raman susceptibility of sugar/water solutions and the calculated VDOS of water in the different lysozyme solutions confirm that sugars induce a significant strengthening of the hydrogen bond network of water that may stabilize proteins at high temperatures.},
	number = {24},
	journal = {The Journal of chemical physics},
	author = {Lerbret, A and Affouard, F and Bordat, P and Hédoux, A and Guinet, Y and Descamps, M},
	month = dec,
	year = {2009},
	keywords = {Animals, Carbohydrates, Carbohydrates: chemistry, Molecular Dynamics Simulation, Muramidase, Muramidase: chemistry, Protein Conformation, Protein Denaturation, raman, Solutions, Spectrum Analysis, Vibration, water, Water: chemistry},
	pages = {245103--245103},
	file = {Lerbret et al. - 2009 - Low-frequency vibrational properties of lysozyme in sugar aqueous solutions a Raman scattering and molecular dyn:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/APHB4MMM/Lerbret et al. - 2009 - Low-frequency vibrational properties of lysozyme in sugar aqueous solutions a Raman scattering and molecular dyn.pdf:application/pdf}
}

@article{kaminski_modeling_2010,
