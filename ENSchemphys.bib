@article{korden_beyond_2012,
	title = {Beyond the {Rosenfeld} {Functional}: {Loop} {Contributions} in {Fundamental} {Measure} {Theory}},
	shorttitle = {Beyond the {Rosenfeld} {Functional}},
	url = {http://arxiv.org/abs/1208.3932},
	abstract = {The Rosenfeld functional provides excellent results for the prediction of the fluid phase of hard convex particle systems but fails beyond the freezing point. The reason for this limitation is the neglect of orientational and distance correlations beyond the particle diameter. In the current article we resolve this restriction and generalize the fundamental measure theory to an expansion in intersection centers. It is shown that the intersection probability of particle systems is described by an algebra, represented by Rosenfeld's weight functions. For subdiagrams of intersection networks we derive vertex functions that provide the building blocks for the free energy functional. Their application is illustrated by deriving the Rosenfeld functional and its leading correction which is exact in the third virial order. Furthermore, the methods are used to derive an approximate functional for the infinite sum over Mayer ring diagrams. Comparing this result to the White Bear mark II functional, we find general agreement between both results.},
	urldate = {2014-11-18},
	journal = {arXiv:1208.3932 [cond-mat]},
	author = {Korden, Stephan},
	month = aug,
	year = {2012},
	note = {arXiv: 1208.3932},
	keywords = {Condensed Matter - Soft Condensed Matter, Condensed Matter - Statistical Mechanics},
	annote = {Comment: 11 figures},
	file = {arXiv\:1208.3932 PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/XRQXFCPA/Korden - 2012 - Beyond the Rosenfeld Functional Loop Contribution.pdf:application/pdf}
}

@article{malozemoff_band-gap_1984,
	title = {"{Band}-gap theory" of strong ferromagnetism: {Application} to concentrated crystalline and amorphous {Fe}- and {Co}-metalloid alloys},
	volume = {29},
	shorttitle = {"{Band}-gap theory" of strong ferromagnetism},
	url = {http://link.aps.org/doi/10.1103/PhysRevB.29.1620},
	doi = {10.1103/PhysRevB.29.1620},
	abstract = {We extend the early work of Stoner, Mott, Friedel, and Terakura and Kanamori, which relates alloy magnetization to solute valence. We describe the conditions under which the simple formula μav=μA0-x(10+ZB-ZA) can be expected to apply. In particular, we consider Fe- and Co-metalloid alloys. Here μav is the atom-averaged moment, μA0 is the host moment, x is the metalloid-atom fraction, and ZB and ZA are the valence of metalloid and transition metal, respectively. We show that the validity of this formula rests on the existence of band gaps in the density of states in the spin-up band. Spin-polarized band-structure calculations do indeed show band gaps in moderately concentrated (x∼0.25) compounds and indicate that μA0 should be somewhat higher for fcc than bcc structures. The theory compares well with data on concentrated amorphous and crystalline alloys of Co with Au, B, Sn, and P, and of Fe with Au, B, Al, Ga, and Si. Our explanation of this large amount of data is far simpler than, and as accurate as, any previous efforts at explanation.},
	number = {4},
	urldate = {2010-03-23},
	journal = {Physical Review B},
	author = {Malozemoff, A. P. and Williams, A. R. and Moruzzi, V. L.},
	year = {1984},
	note = {Copyright (C) 2010 The American Physical Society; Please report any problems to prola@aps.org},
	pages = {1620},
	file = {APS Full Text PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/6DDNRXBT/Malozemoff et al. - 1984 - Band-gap theory of strong ferromagnetism Applic.pdf:application/pdf;APS Full Text PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/83IAZ93Q/Malozemoff et al. - 1984 - Band-gap theory of strong ferromagnetism Applic.pdf:application/pdf;APS Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/RJWPUJPD/p1620_1.html:text/html}
}

@article{michelin-jamois_giant_2015-1,
	title = {Giant {Osmotic} {Pressure} in the {Forced} {Wetting} of {Hydrophobic} {Nanopores}},
	volume = {115},
	url = {http://link.aps.org/doi/10.1103/PhysRevLett.115.036101},
	doi = {10.1103/PhysRevLett.115.036101},
	abstract = {The forced intrusion of water in hydrophobic nanoporous pulverulent material is of interest for quick storage of energy. With nanometric pores the energy storage capacity is controlled by interfacial phenomena. With subnanometric pores, we demonstrate that a breakdown occurs with the emergence of molecular exclusion as a leading contribution. This bulk exclusion effect leads to an osmotic contribution to the pressure that can reach levels never previously sustained. We illustrate, on various electrolytes and different microporous materials, that a simple osmotic pressure law accounts quantitatively for the enhancement of the intrusion and extrusion pressures governing the forced wetting and spontaneous drying of the nanopores. Using electrolyte solutions, energy storage and power capacities can be widely enhanced.},
	number = {3},
	journal = {Physical Review Letters},
	author = {Michelin-Jamois, Millan and Picard, Cyril and Vigier, Gérard and Charlaix, Elisabeth},
	month = jul,
	year = {2015},
	pages = {036101}
}

