	file = {AIP Journal PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/WKP4C57D/Yamaguchi et al. - 2004 - Molecular dynamics simulation study on the transie.pdf:application/pdf;AIP Journal Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/UEMDMEQK/p014512_s1.html:text/html}
