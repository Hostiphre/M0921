	pages = {184503--184503--7},
	file = {Levesque et al. - 2013 - Structure and dynamics in yttrium-based molten rar.pdf:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/5NPHN74J/Levesque et al. - 2013 - Structure and dynamics in yttrium-based molten rar.pdf:application/pdf;supplementary material.pdf:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/7AZGRQ8W/supplementary material.pdf:application/pdf}
