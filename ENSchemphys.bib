	file = {ACS Full Text PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/QAIKJHJ7/Yeh et Hummer - 2004 - System-Size Dependence of Diffusion Coefficients a.pdf:application/pdf}
}

@article{alcor_stochastic_2004,
