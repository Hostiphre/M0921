	pages = {094104},
	file = {Wachowicz et al. - 2010 - Cohesive and magnetic properties of grain boundari.pdf:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/DVEJWXA5/Wachowicz et al. - 2010 - Cohesive and magnetic properties of grain boundari.pdf:application/pdf}
}

