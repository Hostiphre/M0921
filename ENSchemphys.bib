@article{smiatek_polyelectrolyte_2010,
	title = {Polyelectrolyte {Electrophoresis} in {Nanochannels}: {A} {Dissipative} {Particle} {Dynamics} {Simulation}},
	volume = {114},
	issn = {1520-6106},
	shorttitle = {Polyelectrolyte {Electrophoresis} in {Nanochannels}},
	url = {http://dx.doi.org/10.1021/jp100128p},
	doi = {10.1021/jp100128p},
	abstract = {We present mesoscopic dissipative particle dynamics-simulations of polyelectrolyte electrophoresis in confined nanogeometries for varying salt concentration and surface slip conditions. Special attention is given to the influence of electroosmotic flow on the migration of the polyelectrolyte. The effective polyelectrolyte mobility is found to depend strongly on the boundary properties, that is, the slip length and the width of the electric double layer. Analytic expressions for the electroosmotic mobility and the total mobility are derived that are in good agreement with the numerical results. The relevant quantity characterizing the effect of slippage is found to be the dimensionless quantity ??δB, where δB is the slip length, and ??1 is an effective electrostatic screening length at the channel boundaries.},
