	keywords = {[8Fe–7S] cluster, chemical indices, Density functional theory, exchange interaction, Magnetic moment, natural orbital analysis},
	pages = {3288--3302},
	file = {Shoji_et_al-2006-International_Journal_of_Quantum_Chemistry.pdf:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/8WJPFJ8B/Shoji_et_al-2006-International_Journal_of_Quantum_Chemistry.pdf:application/pdf;Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/WWV3VMD6/abstract\;jsessionid=A35CEE26FBA5D24180FB4F5EB16AE004.html:text/html}
