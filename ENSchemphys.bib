@article{olsson_ab_2007,
	title = {Ab initio study of {Cr} interactions with point defects in bcc {Fe}},
	volume = {75},
	url = {http://link.aps.org/doi/10.1103/PhysRevB.75.014110},
	doi = {10.1103/PhysRevB.75.014110},
	abstract = {The properties of Cr in α Fe have been investigated by ab initio calculations based on density functional theory. The intrinsic point defect formation energies were found to be larger in model bcc Cr as compared to those in ferromagnetic bcc Fe. The interactions of Cr with point defects (vacancy and self-interstitials) have been characterized. Single Cr atoms interact weakly with vacancies but significantly with self-interstitial atoms (SIA). Mixed interstitials of any interstitial symmetry are bound. Configurations where two Cr atoms are in nearest-neighbor position are generally unfavorable in bcc Fe except when they are a part of a ⟨111⟩ interstitial complex. Mixed ⟨110⟩ interstitials do not have as strong directional stability as pure Fe interstitials have. The effects on the results using the atom description scheme of either the ultrasoft pseudopotential (USPP) or the projector augmented wave (PAW) formalisms are connected to the differences in local magnetic moments that the two methods predict. As expected for the Fe-Cr system, the results obtained using the PAW method are more reliable than the ones obtained with USPP.},
	number = {1},
	urldate = {2010-03-29},
	journal = {Physical Review B},
	author = {Olsson, P. and Domain, C. and Wallenius, J.},
	month = jan,
	year = {2007},
	note = {Copyright (C) 2010 The American Physical Society; Please report any problems to prola@aps.org},
	pages = {014110},
	file = {Olsson et al. - 2007 - Ab initio study of Cr interactions with point defe.pdf:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/XUF8FS8G/Olsson et al. - 2007 - Ab initio study of Cr interactions with point defe.pdf:application/pdf}
}

