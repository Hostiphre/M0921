	keywords = {Advanced blanket, DCLL, EVOLVE, Flibe, HCPB, Helium, Lead lithium, Review, SCLL},
	pages = {912--919},
	file = {ScienceDirect Full Text PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/8ZEFW85S/Ihli et al. - 2008 - Review of blanket designs for advanced fusion reac.pdf:application/pdf}
