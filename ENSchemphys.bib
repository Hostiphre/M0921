@article{espinosa_fluid-solid_2013,
	title = {On fluid-solid direct coexistence simulations: {The} pseudo-hard sphere model},
	volume = {139},
	issn = {0021-9606, 1089-7690},
	shorttitle = {On fluid-solid direct coexistence simulations},
	url = {http://scitation.aip.org/content/aip/journal/jcp/139/14/10.1063/1.4823499},
	doi = {10.1063/1.4823499},
	abstract = {We investigate methodological issues concerning the direct coexistence method, an increasingly popular approach to evaluate the solid-fluid coexistence by means of computer simulations. The first issue is the impact of the simulation ensemble on the results. We compare the NpT ensemble (easy to use but approximate) with the Np z T ensemble (rigorous but more difficult to handle). Our work shows that both ensembles yield similar results for large systems (\&gt;5000 particles). Another issue, which is usually disregarded, is the stochastic character of a direct coexistence simulation. Here, we assess the impact of stochasticity in the determination of the coexistence point. We demonstrate that the error generated by stochasticity is much larger than that caused by the use of the NpT ensemble, and can be minimized by simply increasing the system size. To perform this study we use the pseudo hard-sphere model recently proposed by Jover et al. [J. Chem. Phys.137, 144505 (2012)], and obtain a coexistence pressure of p* = 11.65(1), quite similar to that of hard spheres (only about 0.6\% higher). Therefore, we conclude that this model can be reliably used to investigate the physics of hard spheres in phenomena like crystal nucleation.},
	number = {14},
	urldate = {2014-01-15},
	journal = {The Journal of Chemical Physics},
	author = {Espinosa, Jorge R. and Sanz, Eduardo and Valeriani, Chantal and Vega, Carlos},
	month = oct,
	year = {2013},
	keywords = {Crystal growth, Free energy, High pressure, Interface dynamics, Interface formation, Liquid solid interfaces, Melting, Nucleation, Numerical modeling, Tensor methods},
	pages = {144502},
	file = {Full Text PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/UVZAD6MJ/Espinosa et al. - 2013 - On fluid-solid direct coexistence simulations The.pdf:application/pdf}
}

