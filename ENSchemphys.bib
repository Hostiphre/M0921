@article{korb_nuclear_2011,
	title = {Nuclear magnetic relaxation of liquids in porous media},
	volume = {13},
	issn = {1367-2630},
	url = {http://iopscience.iop.org/1367-2630/13/3/035016},
	doi = {10.1088/1367-2630/13/3/035016},
	abstract = {Nuclear magnetic relaxation is useful for probing physical and chemical properties of liquids in porous media. Examples are given on high surface area porous materials including calibrated porous silica glasses, granular packings, plaster pastes, cement-based materials and natural porous materials, such as sandstone and carbonate rocks. Here, we outline our recent NMR relaxation work for these very different porous materials. For instance, low field NMR relaxation of water in calibrated granular packings leads to striking different pore-size dependencies of the relaxation times T1 and T2 when changing the amount of surface paramagnetic impurities. This allows separation of the diffusion and surface limited regimes of relaxation in these macroporous media. The magnetic field dependence of the nuclear spin–lattice relaxation rate 1/T1(ω0) is also a rich source of dynamical information for characterizing the molecular dynamics of liquids in porous media. This allows a continuous characterization of the evolving microstructure of various cementitious materials. Our recent applications of two-dimensional (2D) T1–T2 and T2-z-store-T2 correlation experiments have evidenced the water exchange in connected micropores of cement pastes. The direct probing of water adsorption time on a solid surface gives access to an original characterization of the surface nano-wettability of porous plaster pastes. We show that such a parameter depends directly on the physical chemistry of the pore surfaces. Lastly, we outline our recent measurements of wettability in oil/brine/reservoir carbonate rocks.},
	language = {en},
	number = {3},
	urldate = {2013-10-04},
	journal = {New Journal of Physics},
	author = {Korb, J.-P.},
	month = mar,
	year = {2011},
	pages = {035016},
	file = {Full Text PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/G93JKBCH/Korb - 2011 - Nuclear magnetic relaxation of liquids in porous m.pdf:application/pdf;Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/HIERKIEH/035016.html:text/html}
}

@article{treglia_surface_1991,
	title = {Surface segregation near the temperature of bulk phase separation: {Incomplete} wetting in {Cu}({Ag}) alloys},
	volume = {44},
	shorttitle = {Surface segregation near the temperature of bulk phase separation},
	url = {http://link.aps.org/doi/10.1103/PhysRevB.44.5842},
	doi = {10.1103/PhysRevB.44.5842},
	abstract = {Surface segregation in alloys should present drastic modifications near a bulk phase transition. In particular, when the phase diagram exhibits a miscibility gap, one can wonder to what extent surface segregation could be viewed as the first step towards bulk phase separation. We show, in the particular case of very dilute Cu(Ag) alloys, that the actual situation is even more complex. More precisely, using simultaneously an energetic model based on the electronic structure (tight-binding Ising model) and a mean-field approximation formulated as an area-preserving map, we found evidence of incomplete wetting, i.e., a finite succession of layering transitions from almost pure Cu to almost pure Ag planes when the bulk Ag concentration approaches the solubility limit. This theoretical result compares satisfactorily with the experimental surface segregation isotherms derived from kinetics studies using Auger-electron spectroscopy, which indeed exhibit, at least, the first (surface) layering transition. Moreover, the experimentally observed hysteresis between the segregation and dissolution isotherms can be interpreted by taking into account the dependence of the size effect with respect to the surface concentration.},
	number = {11},
	urldate = {2010-08-27},
	journal = {Physical Review B},
	author = {Tréglia, G. and Legrand, B. and Eugène, J. and Aufray, B. and Cabané, F.},
	year = {1991},
	pages = {5842},
	file = {Tréglia et al. - 1991 - Surface segregation near the temperature of bulk p.pdf:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/F79K68HK/Tréglia et al. - 1991 - Surface segregation near the temperature of bulk p.pdf:application/pdf}
}

