@article{mirzoev_calculation_2004,
	title = {Calculation of the energy of mixing for the {Fe}-{Cr} alloys by the first-principles methods of computer simulation},
	volume = {97},
	issn = {0031-918X},
	abstract = {The dependences of the mean magnetic moments per atom and the energy of mixing on the chromium concentration in an iron substitutional solid-solution alloy are considered. An attempt is made to explain the anomalous behaviour of the alloy characteristics (short-range order, Curie temperature, electrical resistance, and crystal-lattice parameter) at low chromium concentrations (less than 10 at. \%) by the fact that the appearance of a chromium atom causes a perturbation of the magnetic structure in a large zone of the alpha-iron lattice surrounding the impurity atom. A cluster strongly differing in its properties from pure iron is brought about by this perturbation. As the concentration is increased, the zones of the magnetic perturbation begin overlap, which leads to a decrease in the energy of chromium-iron interaction in the range of x(Cr) {\textgreater} 0.03. Upon further growth of the chromium concentration, the parameters of the interaction depend only slightly on the concentration.},
	number = {4},
	journal = {PHYSICS OF METALS AND METALLOGRAPHY},
	author = {Mirzoev, A. A. and Yalalov, M. M. and Mirzaev, D. A.},
	year = {2004},
