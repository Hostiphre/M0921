@article{li_nuclear_2008,
	title = {Nuclear {Magnetic} {Shielding} of the 113Cd({II}) {Ion} in {Aqua} {Solution}: {A} {Combined} {Molecular} {Dynamics}/{Density} {Functional} {Theory} {Study}},
