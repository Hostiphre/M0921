	journal = {International Journal of Quantum Chemistry},
	author = {Shoji, M. and Koizumi, K. and Kitagawa, Y. and Yamanaka, S. and Okumura, M. and Yamaguchi, K. and Ohki, Y. and Sunada, Y. and Honda, M. and Tatsumi, K.},
