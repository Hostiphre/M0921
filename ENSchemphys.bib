@article{chandra_molecular_1989,
	title = {A molecular theory of collective orientational relaxation in pure and binary dipolar liquids},
	volume = {91},
	issn = {00219606},
	url = {http://jcp.aip.org/resource/1/jcpsa6/v91/i3/p1829_s1},
	doi = {doi:10.1063/1.457088},
	abstract = {A molecular theory of collective orientational relaxation of dipolar molecules in a dense liquid is presented. Our work is based on a generalized, nonlinear, Smoluchowski equation (GSE) that includes the effects of intermolecular interactions through a mean‐field force term. The effects of translational motion of the liquid molecules on the orientational relaxation is also included self‐consistently in the GSE. Analytic expressions for the wave‐vector‐dependent orientational correlation functions are obtained for one component, pure liquid and also for binary mixtures. We find that for a dipolar liquid of spherical molecules, the correlation function ϕ(k,t) for l=1, where l is the rank of the spherical harmonics, is biexponential. At zero wave‐vector, one time constant becomes identical with the dielectric relaxation time of the polar liquid. The second time constant is the longitudinal relaxation time, but the contribution of this second component is small. We find that polar forces do not affect the higher order correlation functions (l{\textgreater}1) of spherical dipolar molecules in a linearized theory. The expression of ϕ(k,t) for a binary liquid is a sum of four exponential terms. We also find that the wave‐vector‐dependent relaxation times depend strongly on the microscopic structure of the dense liquid. At intermediate wave vectors, the translational diffusion greatly accelerates the rate of orientational relaxation. The present study indicates that one must pay proper attention to the microscopic structure of the liquid while treating the translational effects. An analysis of the nonlinear terms of the GSE is also presented. An interesting coupling between the number density fluctuation and the orientational fluctuation is uncovered.},
	number = {3},
	urldate = {2013-05-13},
	journal = {The Journal of Chemical Physics},
	author = {Chandra, Amalendu and Bagchi, Biman},
	month = aug,
	year = {1989},
	pages = {1829--1842},
	file = {AIP Journal PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/QFVFQ9EN/Chandra et Bagchi - 1989 - A molecular theory of collective orientational rel.pdf:application/pdf;AIP Journal Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/NF9GHJ57/p1829_s1.html:text/html}
}

