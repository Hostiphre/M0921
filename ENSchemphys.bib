	pages = {10048--10054},
	file = {ACS Full Text PDF w/ Links:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/TP653R44/Zakutayev et al. - 2013 - Theoretical Prediction and Experimental Realizatio.pdf:application/pdf}
