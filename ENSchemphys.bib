@article{kloss_treatment_2008,
	title = {Treatment of charged solutes in three-dimensional integral equation theory},
	volume = {128},
	issn = {00219606},
	url = {http://jcp.aip.org/resource/1/jcpsa6/v128/i13/p134505_s1},
	doi = {doi:10.1063/1.2841967},
	abstract = {Periodicity artifacts, which occur within three-dimensional reference interaction site model integral equation theory for net-charged solute systems, are analyzed and corrected by means of a renormalization procedure for long range interactions. The method is formulated for solute-solvent and solute-solute variants of the theory. Both dielectric and electrolyte solvents are considered. Comparison of the results for atomic ions with one-dimensional reference computations shows that structural and thermodynamic artifacts are efficiently removed.},
	number = {13},
	urldate = {2013-01-15},
	journal = {The Journal of Chemical Physics},
	author = {Kloss, Thomas and Kast, Stefan M.},
