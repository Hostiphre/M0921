@article{mazziotti_anti-hermitian_2006,
	title = {Anti-{Hermitian} {Contracted} {Schr}{\textbackslash}"odinger {Equation}: {Direct} {Determination} of the {Two}-{Electron} {Reduced} {Density} {Matrices} of {Many}-{Electron} {Molecules}},
