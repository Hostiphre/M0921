@article{porion_7li_2008,
	title = {7Li {NMR} {Spectroscopy} and {Multiquantum} {Relaxation} as a {Probe} of the {Microstructure} and {Dynamics} of {Confined} {Li}+ {Cations}: {An} {Application} to {Dense} {Clay} {Sediments}},
