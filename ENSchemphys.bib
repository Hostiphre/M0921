	year = {2014},
	keywords = {Determinants, Excitation energies, Quantum Monte Carlo methods, Stochastic processes, Wave functions},
	pages = {244117},
	file = {Full Text PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/B6DPB4FX/Overy et al. - 2014 - Unbiased reduced density matrices and electronic p.pdf:application/pdf;Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/CP6MMGA5/1.html:text/html}
}

