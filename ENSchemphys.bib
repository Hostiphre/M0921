	author = {Simonson, Thomas and Gaillard, Thomas and Mignon, David and Schmidt am Busch, Marcel and Lopes, Anne and Amara, Najette and Polydorides, Savvas and Sedano, Audrey and Druart, Karen and Archontis, Georgios},
	month = oct,
	year = {2013},
	keywords = {aminoacyl-tRNA synthetase, Computer Simulation, molecular recognition, Monte Carlo, protein engineering},
	pages = {2472--2484},
	file = {Full Text PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/GEH254SP/Simonson et al. - 2013 - Computational protein design The proteus software.pdf:application/pdf}
