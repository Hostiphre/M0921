	keywords = {carbon nanotube, collapse, dynamics, electric-field, field-controlled water, hydration   free-energy, hydrophobic hydration, poisson-boltzmann equation, solutes, surface-tension},
	file = {ISI Web of Knowledge Record:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/NVUN8ZFX/full_record.html:text/html}
}

