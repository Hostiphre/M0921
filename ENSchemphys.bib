	journal = {Journal of Molecular Liquids},
	author = {Kinoshita, Masahiro},
	year = {2005},
	keywords = {Density, solute, Water},
	pages = {47--54},
	file = {ScienceDirect Full Text PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/4GVBMNQW/Kinoshita - 2005 - Density and orientational structure of water aroun.pdf:application/pdf;ScienceDirect Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/RAJB4I9D/S0167732204002971.html:text/html}
