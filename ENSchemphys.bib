	pages = {104503--104503--6},
	file = {AIP Journal PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/97FGR9SJ/Videla et al. - 2011 - Aqueous electrolytes confined within functionalize.pdf:application/pdf;AIP Journal Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/Q3X3UIEZ/p104503_s1.html:text/html}
