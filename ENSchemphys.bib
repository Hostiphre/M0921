	pages = {5378--5383},
	file = {Full Text PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/HN3RAW7T/Merzel et Smith - 2002 - Is the first hydration shell of lysozyme of higher.pdf:application/pdf;Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/33C4MPKH/5378.html:text/html}
