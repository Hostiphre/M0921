@article{filippova_mossbauer_2000,
	title = {Mossbauer study of irradiation-accelerated short-range ordering in binary {Fe}-{Cr} alloys},
	volume = {90},
	issn = {0031-918X},
	abstract = {Mossbauer spectroscopy was used to study processes of short-range ordering in Fe-Cr (4, 9, or 16 at. \%) alloys irradiated with electrons at 370 and 420 K. An analysis of the atomic distribution over the first two coordination shells confirmed the existence of a concentration boundary separating two types of short-range ordering, which was found earlier by resistivity measurements and neutron diffraction in the alloys annealed in a temperature range of 700-1000 K. The Fe-4Cr alloy exhibits short-range ordering that is antecedent of long-range order, whereas the Fe-16Cr alloy shows short-range ordering that is antecedent of precipitation (clustering). The composition Fe-9Cr lies near the concentration boundary at which the change in the type of short-range ordering occurs. The processes of short-range ordering upon thermal anneals and irradiation are accompanied by the evolution of the magnetic texture of the samples.},
