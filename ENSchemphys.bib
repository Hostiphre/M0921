@article{ida_direct_2008,
	title = {Direct {NMR} {Detection} of {Alkali} {Metal} {Ions} {Bound} to {G}-{Quadruplex} {DNA}},
	volume = {130},
	issn = {0002-7863},
	url = {http://dx.doi.org/10.1021/ja709975z},
	doi = {10.1021/ja709975z},
	abstract = {We describe a general multinuclear (1H, 23Na, 87Rb) NMR approach for direct detection of alkali metal ions bound to G-quadruplex DNA. This study is motivated by our recent discovery that alkali metal ions (Na+, K+, Rb+) tightly bound to G-quadruplex DNA are actually ?NMR visible? in solution (Wong, A.; Ida, R.; Wu, G. Biochem. Biophys. Res. Commun. 2005, 337, 363). Here solution and solid-state NMR methods are developed for studying ion binding to the classic G-quadruplex structures formed by three DNA oligomers:? d(TG4T), d(G4T3G4), and d(G4T4G4). The present study yields the following major findings. (1) Alkali metal ions tightly bound to G-quadruplex DNA can be directly observed by NMR in solution. (2) Competitive ion binding to the G-quadruplex channel site can be directly monitored by simultaneous NMR detection of the two competing ions. (3) Na+ ions are found to locate in the diagonal T4 loop region of the G-quadruplex formed by two strands of d(G4T4G4). This is the first time that direct NMR evidence has been found for alkali metal ion binding to the diagonal T4 loop in solution. We propose that the loop Na+ ion is located above the terminal G-quartet, coordinating to four guanine O6 atoms from the terminal G-quartet and one O2 atom from a loop thymine base and one water molecule. This Na+ ion coordination is supported by quantum chemical calculations on 23Na chemical shifts. Variable-temperature 23Na NMR results have revealed that the channel and loop Na+ ions in d(G4T4G4) exhibit very different ion mobilities. The loop Na+ ions have a residence lifetime of 220 ?s at 15 °C, whereas the residence lifetime of Na+ ions residing inside the G-quadruplex channel is 2 orders of magnitude longer. (4) We have found direct 23Na NMR evidence that mixed K+ and Na+ ions occupy the d(G4T4G4) G-quadruplex channel when both Na+ and K+ ions are present in solution. (5) The high spectral resolution observed in this study is unprecedented in solution 23Na NMR studies of biological macromolecules. Our results strongly suggest that multinuclear NMR is a viable technique for studying ion binding to G-quadruplex DNA.},
	number = {11},
	urldate = {2012-12-05},
	journal = {Journal of the American Chemical Society},
	author = {Ida, Ramsey and Wu, Gang},
	month = mar,
	year = {2008},
	pages = {3590--3602},
	file = {ACS Full Text PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/WVI7QWZW/Ida et Wu - 2008 - Direct NMR Detection of Alkali Metal Ions Bound to.pdf:application/pdf;ACS Full Text Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/3QCUM9G5/ja709975z.html:text/html}
}

