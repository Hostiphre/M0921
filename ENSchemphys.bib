@article{laage_residence_2008,
	title = {On the {Residence} {Time} for {Water} in a {Solute} {Hydration} {Shell}: {Application} to {Aqueous} {Halide} {Solutions}},
	volume = {112},
	issn = {1520-6106},
	shorttitle = {On the {Residence} {Time} for {Water} in a {Solute} {Hydration} {Shell}},
	url = {http://dx.doi.org/10.1021/jp802033r},
	doi = {10.1021/jp802033r},
	abstract = {We investigate several different methods to determine the water residence time next to a solute from molecular dynamics simulations. The popular computational prescription due to Impey et al. (J. Phys. Chem. 1983, 87, 5071?5083) is shown to be extremely sensitive to the t* tolerance time value (designed to account for barrier recrossing effects), and we evidence through a kinetic analysis that the conventionally employed t* = 2 ps value can yield seriously overestimated residence times for low barrier exchanges. We suggest an alternate, robust determination based on the stable states picture (SSP) of chemical reactions (J. Chem. Phys. 1980, 73, 2700?2714) where recrossing is naturally discarded. This is illustrated by calculation of the water residence time next to a water molecule and next to a chloride ion, using both nonpolarizable and polarizable force-fields. The SSP results are in good agreement with the residence times estimated by a separate kinetic analysis, and differ noticeably from those calculated in the conventional fashion mentioned above.},
	number = {26},
	urldate = {2013-02-15},
	journal = {The Journal of Physical Chemistry B},
	author = {Laage, Damien and Hynes, James T.},
	year = {2008},
	pages = {7697--7701},
	file = {ACS Full Text PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/KQ22G595/Laage et Hynes - 2008 - On the Residence Time for Water in a Solute Hydrat.pdf:application/pdf;ACS Full Text Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/H7RUAQFC/jp802033r.html:text/html}
}

