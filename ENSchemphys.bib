@article{sagaradze_phase_2001,
	title = {Phase transformations in {Fe}-{Cr} alloys upon thermal aging and electron irradiation},
	volume = {92},
	issn = {0031-918X},
	abstract = {The effects of electron irradiation and thermal holding at temperatures in a range of 773-1473 K on the phase transformations and variation of mechanical properties of iron alloys with 5, 9, 13, 18, 28, 35, and 45 wt \% Cr were investigated. Irradiation with electrons or holding at 773 K for 10 h is shown to result in short-range ordering in the alloys with 5 and 9 wt \%, unlike the alloys with 13-45 wt \% Cr, in which chromium segregation with the formation of the alpha' phase occurs. Electron irradiation of the alloys with 13-45 wt \% Cr results in their additional 10-20\% hardening due to the intensification of solid-solution decomposition. The development of internal nitriding of high-chromium alloys with the formation of chromium nitrides is observed upon holdings at high temperatures in air.},
