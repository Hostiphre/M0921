@article{skinner_theory_2011,
	title = {Theory of volumetric capacitance of an electric double-layer supercapacitor},
	volume = {83},
	url = {http://link.aps.org/doi/10.1103/PhysRevE.83.056102},
	doi = {10.1103/PhysRevE.83.056102},
	abstract = {Electric double-layer supercapacitors are a fast-rising class of high-power energy storage devices based on porous electrodes immersed in a concentrated electrolyte or ionic liquid. As yet there is no microscopic theory to describe their surprisingly large capacitance per unit volume (volumetric capacitance) of {\textasciitilde}100 F/cm3, nor is there a good understanding of the fundamental limits on volumetric capacitance. In this paper we present a non-mean-field theory of the volumetric capacitance of a supercapacitor that captures the discrete nature of the ions and the exponential screening of their repulsive interaction by the electrode. We consider analytically and via Monte Carlo simulations the case of an electrode made from a good metal and show that in this case the volumetric capacitance can reach the record values. We also study how the capacitance is reduced when the electrode is an imperfect metal characterized by some finite screening radius. Finally, we argue that a carbon electrode, despite its relatively large linear screening radius, can be approximated as a perfect metal because of its strong nonlinear screening. In this way the experimentally measured capacitance values of {\textasciitilde}100 F/cm3 may be understood.},
	number = {5},
	urldate = {2013-05-03},
	journal = {Physical Review E},
	author = {Skinner, Brian and Chen, Tianran and Loth, M. S. and Shklovskii, B. I.},
	year = {2011},
	pages = {056102},
	file = {APS Full Text PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/UBZJPSJ5/Skinner et al. - 2011 - Theory of volumetric capacitance of an electric do.pdf:application/pdf;APS Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/H673EK4S/e056102.html:text/html}
}

