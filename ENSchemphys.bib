@article{shimizu_temperature_2000,
	title = {Temperature dependence of hydrophobic interactions: {A} mean force perspective, effects of water density, and nonadditivity of thermodynamic signatures},
	volume = {113},
	issn = {0021-9606, 1089-7690},
	shorttitle = {Temperature dependence of hydrophobic interactions},
	url = {http://scitation.aip.org/content/aip/journal/jcp/113/11/10.1063/1.1288922},
	doi = {10.1063/1.1288922},
	abstract = {Temperature-dependent properties of hydrophobic interactions are investigated by simulating the potential of mean force (PMF) between two methane-like solutes in TIP4P model water. Independent results from test particle insertion and free energy perturbation are compared to ensure that zero-PMF baselines are accurate. PMFs are computed under atmospheric pressure at five temperatures from 5 to 95 °C using constant-pressure simulations. The temperature dependence we observe does not agree with previous results from constant-volume simulations, highlighting the important effects of temperature-dependent water density on PMFs. Heat capacity changes upon association of two solutes are estimated at the PMF contact minimum, desolvation barrier, and the solvent (water)-separated minimum. The magnitude of the heat capacity change upon contact formation is much smaller than that predicted by the solvent accessible surface area (SASA). More surprisingly, the heat capacity change upon bringing two methanes from infinity to the desolvation barrier is large and positive. This implies that the thermodynamic signatures of the free energy barrier to desolvation have signs opposite to desolvation itself. This feature is not predicted by either SASA or a volume-based solvent exclusion model. The implications of these and other observations on implicit-solvent model potentials are discussed. Formulations based on thermodynamic perturbation and Widom’s potential distribution theory are developed to relate PMF and hydration mean forces to the underlying structural properties of aqueous solutions. In particular, we provide a theoretical perspective to understand PMF in terms of local water density and the occurrences of configurations with highly unfavorable solute–solvent repulsive interactions.},
	number = {11},
	urldate = {2014-05-07},
	journal = {The Journal of Chemical Physics},
	author = {Shimizu, Seishi and Chan, Hue Sun},
	month = sep,
	year = {2000},
	keywords = {Atmospheric water, Heat capacity, Solution thermodynamics, Solvents, Thermodynamic properties},
	pages = {4683--4700},
	file = {1.1288922.pdf:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/P237VJBR/1.1288922.pdf:application/pdf;Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/PXJ45DV5/1.html:text/html}
}

@article{zhao_assessment_2013,
	title = {Assessment of mesoscopic particle-based methods in microfluidic geometries},
	volume = {139},
	issn = {00219606},
	url = {http://jcp.aip.org/resource/1/jcpsa6/v139/i8/p084109_s1},
	doi = {doi:10.1063/1.4819124},
	abstract = {We assess the accuracy and efficiency of two particle-based mesoscopic simulation methods, namely, Dissipative Particle Dynamics (DPD) and Stochastic Rotation Dynamics (SRD) for predicting a complex flow in a microfluidic geometry. Since both DPD and SRD use soft or weakly interacting particles to carry momentum, both methods contain unavoidable inertial effects and unphysically high fluid compressibility. To assess these effects, we compare the predictions of DPD and SRD for both an exact Stokes-flow solution and nearly exact solutions at finite Reynolds numbers from the finite element method for flow in a straight channel with periodic slip boundary conditions. This flow represents a periodic electro-osmotic flow, which is a complex flow with an analytical solution for zero Reynolds number. We find that SRD is roughly ten-fold faster than DPD in predicting the flow field, with better accuracy at low Reynolds numbers. However, SRD has more severe problems with compressibility effects than does DPD, which limits the Reynolds numbers attainable in SRD to around 25–50, while DPD can achieve Re higher than this before compressibility effects become too large. However, since the SRD method runs much faster than DPD does, we can afford to enlarge the number of grid cells in SRD to reduce the fluid compressibility at high Reynolds number. Our simulations provide a method to estimate the range of conditions for which SRD or DPD is preferable for mesoscopic simulations.},
	number = {8},
	urldate = {2013-08-29},
	journal = {The Journal of Chemical Physics},
	author = {Zhao, Tongyang and Wang, Xiaogong and Jiang, Lei and Larson, Ronald G.},
	month = aug,
	year = {2013},
	pages = {084109--084109--10},
	file = {AIP Journal PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/FBUJ25PX/Zhao et al. - 2013 - Assessment of mesoscopic particle-based methods in.pdf:application/pdf;AIP Journal Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/AXEZJEJE/p084109_s1.html:text/html;Attachment:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/D9IQMDSS/Zhao et al. - 2013 - Assessment of mesoscopic particle-based methods in microfluidic geometries(2).pdf:application/pdf}
}

@article{okamoto_local_2010,
