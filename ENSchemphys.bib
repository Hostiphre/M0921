@article{choi_molecular_2006,
	title = {Molecular dynamics simulations of trehalose as a 'dynamic reducer' for solvent water molecules in the hydration shell.},
	volume = {341},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/16546147},
	doi = {10.1016/j.carres.2006.02.032},
	abstract = {Systematic computational work for a series of 13 disaccharides was performed to provide an atomic-level insight of unique biochemical role of the alpha,alpha-(1--{\textgreater}1)-linked glucopyranoside dimer over the other glycosidically linked sugars. Superior osmotic and cryoprotective abilities of trehalose were explained on the basis of conformational and hydration characteristics of the trehalose molecule. Analyses of the hydration number and radial distribution function of solvent water molecules showed that there was very little hydration adjacent to the glycosidic oxygen of trehalose and that the dynamic conformation of trehalose was less flexible than any of the other sugars due to this anisotropic hydration. The remarkable conformational rigidity that allowed trehalose to act as a sugar template was required for stable interactions with hydrogen-bonded water molecules. Trehalose made an average of 2.8 long-lived hydrogen bonds per each MD step, which was much larger than the average of 2.1 for the other sugars. The stable hydrogen-bond network is derived from the formation of long-lived water bridges at the expense of decreasing the dynamics of the water molecules. Evidence for this dynamic reduction of water by trehalose was also established based on each of the lowest translational diffusion coefficients and the lowest intermolecular coulombic energy of the water molecules around trehalose. Overall results indicate that trehalose functions as a 'dynamic reducer' for solvent water molecules based on its anisotropic hydration and conformational rigidity, suggesting that macroscopic solvent properties could be modulated by changes in the type of glycosidic linkages in sugar molecules.},
	number = {8},
	journal = {Carbohydrate Research},
	author = {Choi, Youngjin and Cho, Kum Won and Jeong, Karpjoo and Jung, Seunho},
	month = jun,
	year = {2006},
	keywords = {Computer Simulation, Hydrogen Bonding, Models, Molecular, Molecular Conformation, Molecular Structure, Solvents, Solvents: chemistry, Trehalose, Trehalose: chemistry, water, Water: chemistry},
	pages = {1020--1028},
	file = {Choi et al. - 2006 - Molecular dynamics simulations of trehalose as a 'dynamic reducer' for solvent water molecules in the hydration she:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/87H2KKBZ/Choi et al. - 2006 - Molecular dynamics simulations of trehalose as a 'dynamic reducer' for solvent water molecules in the hydration she.pdf:application/pdf}
}

