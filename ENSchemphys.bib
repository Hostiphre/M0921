	pages = {10342--10348},
	file = {ACS Full Text PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/FESGVBCJ/Calandra et al. - 2002 - Nuclear Quadrupole Coupling of 17O and 33S in Ioni.pdf:application/pdf;ACS Full Text Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/E93ZSTMV/jp020714r.html:text/html}
}

