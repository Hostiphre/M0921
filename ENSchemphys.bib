@inproceedings{macdonald_role_2001,
	address = {184 THORN HILL RD, WARRENDALE, PA 15086-7514 USA},
	title = {The role of internal/external environment coupling in stress corrosion cracking},
	isbn = {0-87339-478-X},
	abstract = {Strong coupling, as required by the differential aeration hypothesis, has been observed in stress corrosion cracking in a variety of systems, including IGSCC in sensitized Type 304 SS in simulated BWR coolant environments at 288 degreesC and in the caustic cracking of AISI 4340 steel at 70 degreesC. Coupling is manifest as a current flowing from the crack to the external surfaces, where it is consumed by the reduction of a cathodic depolarizer, such as oxygen, water, or hydrogen ion. Examination of this current, which is easily measured using a sensitive zero resistance ammeter, shows that it contains “structured” noise superimposed upon a mean. In the case of stainless steel in the high temperature aqueous environment, the mean current is found to be linearly related to the crack propagation rate and, indeed, the measurement of the coupling current may provide a sensitive method of measuring crack growth rate. Furthermore, the noise in the current is found to yield a wealth of information on the fracture events that occur at the crack tip, including their frequency, temporal relationship with other events, and size. This information has provided a clear view of the fracture mechanisms, which in both cases (IGSCC in stainless steel in BWR environments and caustic cracking in AISI 4340) appear to involve brittle micro fracture events of a few micrometers in size. These data are more consistent with hydrogen-induced fracture than they are with a slip/dissolution mechanism, even though the external environment is oxidizing in nature. Finally, the conservation of charge, as applied to the coupling current, provides the theoretical link between the properties of the crack internal and external environments that is necessary to estimate crack growth rate and hence the accumulated damage in a deterministic manner.},
	booktitle = {{CHEMISTRY} {AND} {ELECTROCHEMISTRY} {OF} {CORROSION} {AND} {STRESS} {CORROSION} {CRACKING}: {A} {SYMPOSIUM} {HONORING} {THE} {CONTRIBUTIONS} {OF} {R}.{W}. {STAEHLE}},
	publisher = {MINERALS, METALS \& MATERIALS SOC},
	author = {Macdonald, D. D.},
	editor = {Jones, R. H.},
	year = {2001},
	note = {Symposium on Chemistry and Electrochemistry of Corrosion and Stress Corrosion Cracking - A Symposium Honoring the Contributions of RW Staehle, NEW ORLEANS, LA, FEB 11-15, 2001},
	pages = {193--210}
}

