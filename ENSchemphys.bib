@article{despinose_de_lacaillerie_mas_2008,
	title = {{MAS} {NMR} spectra of quadrupolar nuclei in disordered solids: {The} {Czjzek} model},
	volume = {192},
	issn = {1090-7807},
	shorttitle = {{MAS} {NMR} spectra of quadrupolar nuclei in disordered solids},
	url = {http://www.sciencedirect.com/science/article/pii/S1090780708000888},
	doi = {10.1016/j.jmr.2008.03.001},
	abstract = {Structural disorder at the scale of two to three atomic positions around the probe nucleus results in variations of the EFG and thus in a distribution of the quadrupolar interaction. This distribution is at the origin of the lineshape tailing toward high fields which is often observed in the MAS NMR spectra of quadrupolar nuclei in disordered solids. The Czjzek model provides an analytical expression for the joint distribution of the NMR quadrupolar parameters υQ and η from which a lineshape can be predicted. This model is derived from the Central Limit Theorem and the statistical isotropy inherent to disorder. It is thus applicable to a wide range of materials as we have illustrated for 27Al spectra on selected examples of glasses (slag), spinels (alumina), and hydrates (cement aluminum hydrates). In particular, when relevant, the use of the Czjzek model allows a quantitative decomposition of the spectra and an accurate extraction of the second moment of the quadrupolar product. In this respect, it is important to realize that only rotational invariants such as the quadrupolar product can make sense to describe the quadrupolar interaction in disordered solids.},
	number = {2},
	urldate = {2013-10-28},
	journal = {Journal of Magnetic Resonance},
	author = {d’Espinose de Lacaillerie, Jean-Baptiste and Fretigny, Christian and Massiot, Dominique},
	year = {2008},
	keywords = {Aluminum-27, Amorphous materials, Disorder, Electrical field gradient, Gel, Glass, Lineshape, Quadrupolar nuclei, Solid-state NMR},
	pages = {244--251},
	file = {ScienceDirect Full Text PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/WTC6896R/d’Espinose de Lacaillerie et al. - 2008 - MAS NMR spectra of quadrupolar nuclei in disordere.pdf:application/pdf;ScienceDirect Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/CPZA9EM7/S1090780708000888.html:text/html}
}

