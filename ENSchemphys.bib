@article{bonthuis_theory_2011,
	title = {Theory and simulations of water flow through carbon nanotubes: prospects and pitfalls},
	volume = {23},
	issn = {0953-8984},
	shorttitle = {Theory and simulations of water flow through carbon nanotubes},
	url = {http://iopscience.iop.org/0953-8984/23/18/184110},
	doi = {10.1088/0953-8984/23/18/184110},
	abstract = {We study water flow through carbon nanotubes using continuum theory and molecular dynamics simulations. The large slip length in carbon nanotubes greatly enhances the pumping and electrokinetic energy conversion efficiency. In the absence of mobile charges, however, the electro-osmotic flow vanishes. Uncharged nanotubes filled with pure water can therefore not be used as electric field-driven pumps, contrary to some recently ventured ideas. This is in agreement with results from a generalized hydrodynamic theory that includes the angular momentum of rotating dipolar molecules. The electro-osmotic flow observed in simulations of such carbon nanotubes is caused by an imprudent implementation of the Lennard-Jones cutoff. We also discuss the influence of other simulation parameters on the spurious electro-osmotic flow.},
