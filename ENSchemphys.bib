	pages = {73--83},
	file = {Meslin et al. - 2010 - Characterization of neutron-irradiated ferritic mo.pdf:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/KFESAZVR/Meslin et al. - 2010 - Characterization of neutron-irradiated ferritic mo.pdf:application/pdf}
