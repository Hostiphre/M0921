@article{yu_numerical_2010,
	title = {A numerical simulation method for dissolution in porous and fractured media},
	volume = {229},
	issn = {0021-9991},
	url = {http://www.sciencedirect.com/science/article/pii/S0021999110002573},
	doi = {10.1016/j.jcp.2010.05.005},
	abstract = {We describe an algorithm for simulating reactive flows in porous media, in which the pore space is mapped explicitly. Chemical reactions at the solid–fluid boundaries lead to dissolution (or precipitation), which makes it necessary to track the movement of the solid–fluid interface during the course of the simulation. We have developed a robust algorithm for constructing a piecewise continuous (C1) surface, which enables a rapid remapping of the surface to the grid lines. The key components of the physics are the Navier–Stokes equations for fluid flow in the pore space, the convection–diffusion equation to describe the transport of chemical species, and rate equations to model the chemical kinetics at the solid surfaces. A lattice-Boltzmann model was used to simulate fluid flow in the pore space, with linear interpolation at the solid boundaries. A finite-difference scheme for the concentration field was developed, taking derivatives along the direction of the local fluid velocity. When the flow is not aligned with the grid this leads to much more accurate convective fluxes and surface concentrations than a standard Cartesian template. A robust algorithm for the surface reaction rates has been implemented, avoiding instabilities when the surface is close to a grid point. We report numerical tests of different aspects of the algorithm and assess the overall convergence of the method.},
	number = {18},
	urldate = {2014-02-21},
	journal = {Journal of Computational Physics},
	author = {Yu, D. and Ladd, A. J. C.},
	month = sep,
	year = {2010},
	keywords = {Dissolution, Finite-difference, Lattice-Boltzmann, Porous media},
	pages = {6450--6465},
	file = {ScienceDirect Full Text PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/U36F37NV/Yu et Ladd - 2010 - A numerical simulation method for dissolution in p.pdf:application/pdf}
}

