@inproceedings{suter_effect_2001,
	address = {184 THORN HILL RD, WARRENDALE, PA 15086-7514 USA},
	title = {The effect of mechanical stress on localized corrosion},
	isbn = {0-87339-478-X},
	abstract = {Corrosion pits on passive metals such as stainless steels are often initiation sites for stress corrosion cracking (SCC). In this work the effect of mechanical stress on the onset of pitting corrosion was investigated, using microcapillaries as electrochemical cells. Pit initiation at MnS inclusions on 304 SS in 1 M NaCl was studied with and without applied mechanical stress. Polarization curves of areas without inclusions showed no pitting at potentials below that of oxidation evolution; stress had no effect on the corrosion behavior. Areas containing four to five small, round inclusions showed stable pitting dr about 400 mV (SCE); applied stress shifted the pitting potential to values that were 150 mV more negative. Areas containing a single large, shallow MnS inclusion showed multiple current transients during dissolution of the inclusion without stress, but the metastable events did not initiate stable pitting. The dissolution of shallow MnS inclusion did not form a deep microcrevice between the MnS and stainless steel matrix. However, under applied stress, cracks were formed within the shallow MnS inclusion. Scanning electron microscopy investigations showed that active pitting started at the bottom of the formed cracks. Due to mechanical stress microcrevices are formed at or within the inclusions, where the concentrations of aggressive species such as chloride or hydrogen ions reaches a critical value for stable pit propagation. Therefore mechanical stress accelerates the onset of pitting and in this way the formation of potential starting sites for stress corrosion cracking. The chemistry inside a microcrevice was simulated using a 1-D finite difference model. Simulations indicated that in a 3 mum deep microcrevice the combination of pH 2 and a chloride concentration of 6 M lead to stable pitting.},
	booktitle = {{CHEMISTRY} {AND} {ELECTROCHEMISTRY} {OF} {CORROSION} {AND} {STRESS} {CORROSION} {CRACKING}: {A} {SYMPOSIUM} {HONORING} {THE} {CONTRIBUTIONS} {OF} {R}.{W}. {STAEHLE}},
	publisher = {MINERALS, METALS \& MATERIALS SOC},
	author = {Suter, T. and Webb, E. G. and Bohni, H. and Alkire, R. C.},
	editor = {Jones, R. H.},
	year = {2001},
	note = {Symposium on Chemistry and Electrochemistry of Corrosion and Stress Corrosion Cracking - A Symposium Honoring the Contributions of RW Staehle, NEW ORLEANS, LA, FEB 11-15, 2001},
	pages = {301--317}
}

