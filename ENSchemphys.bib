	file = {ACS Full Text PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/ZUX4D2K9/Coalson and Duncan - 1996 - Statistical Mechanics of a Multipolar Gas  A Latt.pdf:application/pdf}
