	month = nov,
	year = {1979},
	pages = {25--39},
	file = {little2.pdf:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/SG5WR2RG/little2.pdf:application/pdf}
}

