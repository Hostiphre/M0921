@article{mccoy_density_1987,
	title = {A density functional treatment of the hard dumbbell freezing transition},
	volume = {87},
	issn = {00219606},
	url = {http://jcp.aip.org/resource/1/jcpsa6/v87/i8/p4853_s1},
