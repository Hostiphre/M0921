	issn = {0021-9606, 1089-7690},
	url = {http://scitation.aip.org.ezproxy.fkf.mpg.de/content/aip/journal/jcp/134/13/10.1063/1.3569134},
	doi = {10.1063/1.3569134},
	abstract = {Quantum Monte Carlo calculations with the diffusion Monte Carlo (DMC) method have been used to compute the binding energy curves of hydrogen on benzene, coronene, and graphene. The DMC results on benzene agree with both Møller–Plessett second order perturbation theory (MP2) and coupled cluster with singles, doubles, and perturbative triples [CCSD(T)] calculations, giving an adsorptionenergy of ∼25 meV. For coronene, DMC agrees well with MP2, giving an adsorptionenergy of ∼40 meV. For physisorbed hydrogen on graphene, DMC predicts a very small adsorptionenergy of only 5 ± 5 meV. Density functional theory(DFT) calculations with various exchange-correlation functionals, including van der Waals corrected functionals, predict a wide range of binding energies on all three systems. The present DMC results are a step toward filling the gap in accurate benchmark data on weakly bound systems. These results can help us to understand the performance of current DFT based methods, and may aid in the development of improved approaches.},
	number = {13},
	urldate = {2015-12-18},
	journal = {The Journal of Chemical Physics},
	author = {Ma, Jie and Michaelides, Angelos and Alfè, Dario},
	month = apr,
	year = {2011},
	keywords = {Adsorption, Carbon, Density functional theory, Graphene, Hydrogen energy},
	pages = {134701},
	file = {Full Text PDF:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/MPH2XPH5/Ma et al. - 2011 - Binding of hydrogen on benzene, coronene, and grap.pdf:application/pdf;Snapshot:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/7M2HK4V4/1.html:text/html}
}

