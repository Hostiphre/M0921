@article{cordone_internal_2005,
	title = {Internal dynamics and protein-matrix coupling in trehalose-coated proteins.},
	volume = {1749},
	issn = {0916234215},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/15886079},
	doi = {10.1016/j.bbapap.2005.03.004},
	abstract = {We review recent studies on the role played by non-liquid, water-containing matrices on the dynamics and structure of embedded proteins. Two proteins were studied, in water-trehalose matrices: a water-soluble protein (carboxy derivative of horse heart myoglobin) and a membrane protein (reaction centre from Rhodobacter sphaeroides). Several experimental techniques were used: Mossbauer spectroscopy, elastic neutron scattering, FTIR spectroscopy, CO recombination after flash photolysis in carboxy-myoglobin, kinetic optical absorption spectroscopy following pulsed and continuous photoexcitation in Q(B) containing or Q(B) deprived reaction centre from R. sphaeroides. Experimental results, together with the outcome of molecular dynamics simulations, concurred to give a picture of how water-containing matrices control the internal dynamics of the embedded proteins. This occurs, in particular, via the formation of hydrogen bond networks that anchor the protein surface to the surrounding matrix, whose stiffness increases by lowering the sample water content. In the conclusion section, we also briefly speculate on how the protein-matrix interactions observed in our samples may shed light on the protein-solvent coupling also in liquid aqueous solutions.},
	number = {2},
	journal = {Biochimica et biophysica acta},
	author = {Cordone, Lorenzo and Cottone, Grazia and Giuffrida, Sergio and Palazzo, Gerardo and Venturoli, Giovanni and Viappiani, Cristiano},
	month = jun,
	year = {2005},
	keywords = {Computer Simulation, Fourier Transform Infrared, Heme, Heme: chemistry, Hydrogen Bonding, Kinetics, Models, Molecular, Myoglobin, Myoglobin: chemistry, Photolysis, Photosynthetic Reaction Center Complex Proteins, Photosynthetic Reaction Center Complex Proteins: c, Rhodobacter sphaeroides, Spectroscopy, Temperature, Trehalose, Trehalose: chemistry, water, Water: chemistry},
	pages = {252--281},
	file = {Cordone et al. - 2005 - Internal dynamics and protein-matrix coupling in trehalose-coated proteins:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/STG6X6EQ/Cordone et al. - 2005 - Internal dynamics and protein-matrix coupling in trehalose-coated proteins.pdf:application/pdf}
}

@article{green_water_1990,
	title = {Water and solutions at negative pressure: {Raman} spectroscopic study to -80 megapascals.},
	volume = {249},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/17831957},
	doi = {10.1126/science.249.4969.649},
	abstract = {Microscopic inclusions of aqueous fluids trapped in interstices in quartz and other crystals provide novel systems for the deliberate study of liquids under tension. Liquids under tension should differ in interesting ways from those at ambient pressure or compressed liquids because attractive, rather than repulsive, forces should dominate their behavior. Static tensions in excess of 100 megapascals ({\textasciitilde}1000 atmospheres) have been obtained reproducibly. Video-recorded observations of the final liquid rupture process, coupled with extrapolations of data at positive pressure, suggest that the homogeneous vapor nucleation point was reached in two of the cases studied. Raman spectra of the fluids at -80 megapascals show that an isothermal volume stretch of -5 percent by volume has only a weak effect on the spectral features and is similar to the effect of isobaric heating.},
	number = {4969},
	journal = {Science},
	author = {Green, J L and Durben, D J and Wolf, G H and Angell, C a},
	month = aug,
	year = {1990},
	pages = {649--652},
	file = {Green et al. - 1990 - Water and solutions at negative pressure Raman spectroscopic study to -80 megapascals:/Users/lding/Library/Application Support/Zotero/Profiles/pp5nulh6.default/zotero/storage/TQHW3RBQ/Green et al. - 1990 - Water and solutions at negative pressure Raman spectroscopic study to -80 megapascals.pdf:application/pdf}
}

